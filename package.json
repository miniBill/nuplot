{
    "browserslist": {
        "production": [
            "> 2%",
            "last 3 and_chr versions",
            "last 3 chrome versions",
            "last 3 opera versions",
            "last 3 ios_saf versions",
            "last 3 safari versions",
            "last 3 firefox versions"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version"
        ]
    },
    "devDependencies": {
        "elm-codegen": "^0.5.3",
        "elm-optimize-level-2": "^0.3.5",
        "elm-review": "^2.11.1",
        "elm-watch": "^2.0.0-beta.2",
        "typescript": "^5.4.4"
    },
    "scripts": {
        "build": "make build",
        "buildForBenchmark": "rm -rf .cache elm-stuff dist; elm-optimize-level-2 src/elm/UI.elm --output src/optimized.js && yarn parcel build --no-minify src/index.html --public-url .",
        "start": "yarn parcel src/dev.html",
        "test": "elm-test"
    },
    "name": "nuplot",
    "description": "A small CAS and plotter",
    "license": "MIT",
    "pwaManifest": {
        "name": "NuPlot",
        "shortName": "NuPlot",
        "startURL": "./index.html",
        "theme": "#dcd0ff",
        "generateIconOptions": {
            "baseIcon": "./public/icon.svg",
            "sizes": [
                192,
                384,
                512
            ],
            "genFavicons": true,
            "purpose": [
                "any",
                "maskable"
            ]
        },
        "display": "standalone",
        "development": {
            "disabled": true
        }
    },
    "dependencies": {
        "localforage": "^1.9.0"
    }
}
